{
  "name": "cinnaceylon-fullstack",
  "version": "1.0.0",
  "description": "CinnaCeylon Full Stack Application",
  "scripts": {
    "install-all": "npm install && cd Cinna-Ceylon/backend && npm install && cd ../frontend && npm install",
    "backend": "cd Cinna-Ceylon/backend && npm run dev",
    "frontend": "cd Cinna-Ceylon/frontend && npm start",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "cd Cinna-Ceylon/frontend && npm run build",
    "start:all": "concurrently \"npm --prefix Cinna-Ceylon/backend start\" \"npm --prefix Cinna-Ceylon/frontend start\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "cinnamon",
    "ceylon",
    "fullstack",
    "react",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2"
  }
}
